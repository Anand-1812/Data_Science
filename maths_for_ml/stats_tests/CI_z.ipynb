   "source": [
    "import numpy as npy\n",
    "import scipy.stats as stat\n",
    "\n",
    "mean = 4.63\n",
    "sdev = 0.54\n",
    "n = 50\n",
    "z = 1.960\n",
    "\n",
    "se = sdev / npy.sqrt(n)\n",
    "moe = z * se\n",
    "\n",
    "lower = mean - moe\n",
    "upper = mean + moe\n",
    "\n",
    "print(f\"Confidence Interval: ({lower:.2f}, {upper:.2f})\")"
   ]
  }
