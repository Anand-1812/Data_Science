   "source": [
    "import math\n",
    "import scipy.stats as stat\n",
    "\n",
    "mean = 240\n",
    "sdev = 25\n",
    "n = 10\n",
    "\n",
    "df = n-1\n",
    "alpha = 0.025\n",
    "\n",
    "t_value = stat.t.ppf(1-alpha, df)\n",
    "moe = t_value * (sdev / math.sqrt(n))\n",
    "\n",
    "lower = mean - moe\n",
    "upper = mean + moe\n",
    "\n",
    "print(f\"Confidence Interval: ({lower:.2f}, {upper:.2f})\")"
   ]
  }
